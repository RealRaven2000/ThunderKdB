{
  "manifest_version": 2,
  "name": "Phoenity Icons",
  "description": "Phoenity Icons for Thunderbird",
  "homepage_url": "https://addons.thunderbird.net/en-US/thunderbird/addon/phoenity-icons/",
  "version": "3.0b1",
  "experiment_apis": {
    "phoenityIconsApi": {
      "schema": "schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          [
            "phoenityIconsApi"
          ]
        ],
        "script": "phoenityIconsApi.js"
      }
    }
  },
  "background": {
    "scripts": [
      "phoenityIcons.js"
    ]
  },
  "applications": {
    "gecko": {
      "id": "icons@dillinger",
      "strict_min_version": "78.0"
    }
  },
  "icons": {
    "32": "phoenityIcons32.png",
    "64": "phoenityIcons64.png"
  }
}
