<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:name>Get Partial Messages</em:name>
    <em:version>0.3.3</em:version>
    <em:description>Get the rest of all partially downloaded messages from server.</em:description>
    <em:creator>Joachim Herb</em:creator>
    <em:id>{6C3323F8-7328-11DE-B11C-551356D89593}</em:id>
    <em:type>2</em:type>
    <em:homepageURL>http://getpartialmessages.mozdev.org</em:homepageURL>

    <em:targetApplication>
      <!-- Postbox -->
      <Description>
        <em:id>postbox@postbox-inc.com</em:id>
        <em:minVersion>2.0b1</em:minVersion>
        <em:maxVersion>2.*</em:maxVersion>
      </Description>
      <!-- Thunderbird -->
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>2.0</em:minVersion>
        <em:maxVersion>38.0</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:iconURL>chrome://getpartialmessages/skin/part_get.png</em:iconURL>
  </Description>
</RDF>
